#include <stdio.h>
int main (){
    int arr[3][3]; 
    int sum=0; 
    int sum2=0; 
    
    printf("Input elemts for 3 by 3 matrix= "); 
    for(int i=0 ; i<3 ; i++){
        for(int j=0 ; j<3 ; j++){
            scanf("%d" ,&arr[i][j]); 
            
         if( i == j ) {
             sum+= arr[i][j]; 
         }  
         }
    }
    printf("The Sum Primary of Diagonal elements is: %d" ,sum);
    return 0; 
}
